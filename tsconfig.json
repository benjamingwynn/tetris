{
	"compilerOptions": {
		"composite": true,
		"target": "ESNext",
		"module": "NodeNext",
		"paths": {},
		"isolatedModules": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipDefaultLibCheck": true,
		"skipLibCheck": true,
		"allowImportingTsExtensions": true,
		"noEmit": true
	},
	"include": ["src", "./*.ts"],
	"references": []
}
