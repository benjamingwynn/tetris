{
	"compilerOptions": {
		"lib": ["WebWorker", "ESNext"],
		"composite": true,
		"target": "ESNext",
		"module": "NodeNext",
		"paths": {},
		"isolatedModules": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipDefaultLibCheck": true,
		"skipLibCheck": true,
		"allowImportingTsExtensions": true,
		"resolveJsonModule": true,
		"noEmit": true
	},
	"include": ["."],
	"references": []
}
